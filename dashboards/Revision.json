          "metricColumn": "none",
          "orderByCol": "1",
          "orderBySort": "1",
          "rawQuery": true,
          "rawSql": "/* Massage */\nSELECT\n    id,\n    origin,\n    description,\n    architecture,\n    config_name,\n    IF(start_time is NULL, \"\", CAST(start_time AS STRING)) as start_time_str,\n    IF(duration is NULL, \"\", FORMAT_TIME(\"%H:%M:%E3S\", TIME_ADD(TIME \"00:00:00\", INTERVAL CAST((duration * 1000) AS INT64) MILLISECOND))) as duration_str,\n    valid_num,\n    tests_status_num\nFROM (\n    /* Aggregate and join */\n    SELECT\n        builds.id AS id,\n        ANY_VALUE(builds.origin) AS origin,\n        ANY_VALUE(builds.description) AS description,\n        ANY_VALUE(builds.architecture) AS architecture,\n        ANY_VALUE(builds.config_name) AS config_name,\n        MIN(builds.start_time) AS start_time,\n        MIN(builds.duration) AS duration,\n        MIN(CASE builds.valid WHEN true THEN 1 WHEN false THEN 0 ELSE -1 END) AS valid_num,\n        MAX((CASE tests.status WHEN \"SKIP\" THEN 1 WHEN \"DONE\" THEN 2 WHEN \"PASS\" THEN 3 WHEN \"FAIL\" THEN 4 WHEN \"ERROR\" THEN 5 ELSE 0 END) -\n            (CASE tests.waived WHEN false THEN 0 WHEN true THEN 1 ELSE 2 END) * 6) as tests_status_num\n    FROM ${dataset}.builds AS builds\n    LEFT OUTER JOIN ${dataset}.tests AS tests ON tests.build_id = builds.id\n    WHERE\n        revision_id = \"$id\" AND\n        builds.origin LIKE ${origin} AND\n        (\n            builds.start_time >= TIMESTAMP_MILLIS(${__from}) AND builds.start_time <= TIMESTAMP_MILLIS(${__to}) OR\n            builds.start_time IS NULL\n        )\n    GROUP BY id\n    ORDER BY start_time ASC\n)",
