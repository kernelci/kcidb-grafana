          "orderBySort": "1",
          "rawQuery": true,
          "rawSql": "SELECT\n    id,\n    IF(ARRAY_LENGTH(origins) > 1, \". . . . .\", origins[SAFE_OFFSET(0)]) AS origin_str,\n    IF(git_commit_name IS NULL, \"\", git_commit_name) AS git_commit_name_str,\n    IF(git_commit_hash IS NULL, \"\", SUBSTR(git_commit_hash, 0, 12)) AS git_commit_hash_short_str,\n    IF(discovery_time IS NULL, \"\", CAST(discovery_time AS STRING)) AS discovery_time_str,\n    valid_num,\n    builds_valid_num,\n    tests_status_num\nFROM (\n    SELECT revisions.id AS id,\n           ARRAY_AGG(DISTINCT revisions.origin) AS origins,\n           ANY_VALUE(revisions.git_commit_name) AS git_commit_name,\n           ANY_VALUE(revisions.git_commit_hash) AS git_commit_hash,\n           MIN(revisions.discovery_time) AS discovery_time,\n           ANY_VALUE(revisions.valid_num) valid_num,\n           MIN(CASE builds.valid WHEN true THEN 1 WHEN false THEN 0 ELSE -1 END) AS builds_valid_num,\n           MAX((CASE tests.status WHEN \"SKIP\" THEN 1 WHEN \"DONE\" THEN 2 WHEN \"PASS\" THEN 3 WHEN \"FAIL\" THEN 4 WHEN \"ERROR\" THEN 5 ELSE 0 END) -\n               (CASE tests.waived WHEN false THEN 0 WHEN true THEN 1 ELSE 2 END) * 6) AS tests_status_num\n    FROM (\n        SELECT id,\n               origin,\n               git_commit_name,\n               git_commit_hash,\n               discovery_time,\n               CASE valid WHEN true THEN 0 WHEN false THEN 1 ELSE -1 END AS valid_num,\n        FROM ${dataset}.revisions\n        WHERE origin LIKE ${origin} AND\n              ${git_repository_url:json} = git_repository_url AND\n              ${git_repository_branch:json} = git_repository_branch AND\n              IFNULL(discovery_time >= TIMESTAMP_MILLIS(${__from}) AND discovery_time <= TIMESTAMP_MILLIS(${__to}), TRUE)\n    ) AS revisions\n    LEFT OUTER JOIN ${dataset}.builds AS builds ON\n      builds.revision_id = revisions.id AND\n      IFNULL(builds.architecture LIKE ${build_architecture}, TRUE) AND\n      IFNULL(builds.config_name LIKE ${build_config_name}, TRUE)\n    LEFT OUTER JOIN ${dataset}.tests AS tests ON tests.build_id = builds.id\n    GROUP BY revisions.id\n    ORDER BY discovery_time DESC\n)",
