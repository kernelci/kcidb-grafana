          "rawSql": "WITH filtered_checkouts AS NOT MATERIALIZED (\n    SELECT\n        checkouts.id,\n        checkouts.origin,\n        checkouts.start_time\n    FROM\n        checkouts\n    WHERE\n        checkouts.git_repository_url = '$git_repository_url'\n),\nfiltered_builds AS NOT MATERIALIZED (\n    SELECT\n        builds.id,\n        builds.origin,\n        builds.start_time,\n        builds.architecture,\n        builds.config_name\n    FROM builds\n    INNER JOIN filtered_checkouts ON filtered_checkouts.id = builds.checkout_id\n),\nfiltered_tests AS NOT MATERIALIZED (\n    SELECT\n        tests.id,\n        tests.origin,\n        tests.start_time\n    FROM tests\n    INNER JOIN filtered_builds ON filtered_builds.id = tests.build_id\n)\nSELECT\n    COALESCE(checkouts.time, builds.time, tests.time) AS time,\n    COALESCE(checkouts.count, 0) AS Checkouts,\n    COALESCE(builds.count, 0) AS Builds,\n    COALESCE(tests.count, 0) AS Tests\nFROM (\n    SELECT\n        date_bin(($__interval_ms * 50 || ' milliseconds')::interval,\n                 start_time, TO_TIMESTAMP($__from / 1000)) AS time,\n        COUNT(id) AS count\n    FROM\n        filtered_checkouts\n    WHERE\n        ($origin = '%' or origin LIKE $origin) AND\n        start_time BETWEEN TO_TIMESTAMP($__from / 1000) AND TO_TIMESTAMP($__to / 1000)\n    GROUP BY\n        time\n) AS checkouts\nFULL OUTER JOIN (\n    SELECT\n        date_bin(($__interval_ms * 50 || ' milliseconds')::interval,\n                 start_time, TO_TIMESTAMP($__from / 1000)) AS time,\n        COUNT(id) AS count\n    FROM filtered_builds\n    WHERE\n        ($origin = '%' or origin LIKE $origin) AND\n    \tstart_time BETWEEN TO_TIMESTAMP($__from / 1000) AND TO_TIMESTAMP($__to / 1000) AND\n        ($build_architecture = '%' OR architecture LIKE $build_architecture) AND\n        ($build_config_name = '%' OR config_name LIKE $build_config_name)\n    GROUP BY\n        time\n) AS builds ON builds.time = checkouts.time\nFULL OUTER JOIN (\n    SELECT\n        date_bin(($__interval_ms * 50 || ' milliseconds')::interval,\n                 start_time, TO_TIMESTAMP($__from / 1000)) AS time,\n        COUNT(id) AS count\n    FROM filtered_tests\n    WHERE\n        ($origin = '%' or origin LIKE $origin) AND\n    \tstart_time BETWEEN TO_TIMESTAMP($__from / 1000) AND TO_TIMESTAMP($__to / 1000)\n    GROUP BY\n        time\n) AS tests ON tests.time = COALESCE(checkouts.time, builds.time)\nORDER BY\n    time",
