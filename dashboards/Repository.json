          "metricColumn": "none",
          "rawQuery": true,
          "rawSql": "WITH filtered_checkouts AS (\n    SELECT\n        git_repository_branch,\n        git_commit_hash,\n        patchset_hash\n    FROM checkouts\n    WHERE\n        start_time BETWEEN TO_TIMESTAMP($__from / 1000) AND TO_TIMESTAMP($__to / 1000) AND\n        git_repository_url = '$git_repository_url' AND\n        git_repository_branch IS NOT NULL AND\n        git_commit_hash IS NOT NULL AND\n        patchset_hash IS NOT NULL AND\n        origin LIKE $origin\n)\nSELECT\n    CASE WHEN $origin = '%' THEN 'All' ELSE $origin END AS origin_param,\n    CASE WHEN $build_architecture = '%' THEN 'All' ELSE $build_architecture END AS build_architecture_param,\n    CASE WHEN $build_config_name = '%' THEN 'All' ELSE $build_config_name END AS build_config_name_param,\n    git_repository_branch AS name,\n    COALESCE(revisions, 0) AS revisions\nFROM (\n    SELECT DISTINCT git_repository_branch FROM filtered_checkouts\n) AS distinct_branches\nLEFT OUTER JOIN (\n    SELECT \n        git_repository_branch, \n        COUNT(DISTINCT CONCAT(git_commit_hash, patchset_hash)) AS revisions\n    FROM filtered_checkouts\n    GROUP BY\n        git_repository_branch\n) AS branch_revisions USING(git_repository_branch)\nORDER BY revisions DESC, name ASC",
